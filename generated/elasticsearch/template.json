{
  "_meta": {
    "description": "Sample composable template that includes all ECS fields",
    "ecs_version": "2.0.0-dev"
  },
  "composed_of": [
    "ecs_2.0.0-dev_foo",
    "ecs_2.0.0-dev_base",
    "ecs_2.0.0-dev_ecs",
    "ecs_2.0.0-dev_event",
    "ecs_2.0.0-dev_http",
    "ecs_2.0.0-dev_network",
    "ecs_2.0.0-dev_related",
    "ecs_2.0.0-dev_agent"
  ],
  "index_patterns": [
    "try-ecs-*"
  ],
  "priority": 1,
  "template": {
    "mappings": {
      "date_detection": false,
      "dynamic_templates": [
        {
          "strings_as_keyword": {
            "mapping": {
              "ignore_above": 1024,
              "type": "keyword"
            },
            "match_mapping_type": "string"
          }
        }
      ]
    },
    "settings": {
      "index": {
        "mapping": {
          "total_fields": {
            "limit": 2000
          }
        }
      }
    }
  }
}