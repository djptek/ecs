{
  "_meta": {
    "documentation": "https://www.elastic.co/guide/en/ecs/current/ecs-acme.html",
    "ecs_version": "2.0.0-dev"
  },
  "template": {
    "mappings": {
      "properties": {
        "acme": {
          "properties": {
            "account": {
              "properties": {
                "id": {
                  "ignore_above": 1024,
                  "type": "keyword"
                }
              }
            }
          }
        }
      }
    }
  }
}